<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_home">
    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.advisingv2.HomeTabsFragment"
        android:label="home"
        tools:layout="@layout/fragment_home_tabs"/>
    <fragment
        android:id="@+id/nav_form"
        android:name="com.example.advisingv2.ui.CourseRequestFragment"
        android:label="course request"
        tools:layout="@layout/fragment_course_request" />
    <fragment
        android:id="@+id/overviewFragment"
        android:name="com.example.advisingv2.ui.OverviewFragment"
        android:label="fragment_overview"
        tools:layout="@layout/fragment_overview" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.example.advisingv2.ui.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/degreerequirementsFragment"
        android:name="com.example.advisingv2.ui.DegreerequirementsFragment"
        android:label="fragment_degreerequirements"
        tools:layout="@layout/fragment_degreerequirements" />
    <fragment
        android:id="@+id/transferInformationFragment"
        android:name="com.example.advisingv2.ui.TransferInformationFragment"
        android:label="fragment_transfer_information"
        tools:layout="@layout/fragment_transfer_information" />
    <navigation android:id="@+id/navigation"
        app:startDestination="@id/nav_settings">
        <fragment
            android:id="@+id/nav_settings"
            android:name="com.example.advisingv2.ui.SettingsFragment"
            android:label="SettingsFragment" />
    </navigation>
    <fragment
        android:id="@+id/courseFragment"
        android:name="com.example.advisingv2.CourseFragment"
        android:label="fragment_course_list"
        tools:layout="@layout/fragment_course_list" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.example.advisingv2.ui.SettingsFragment"
        android:label="SettingsFragment" />
</navigation>